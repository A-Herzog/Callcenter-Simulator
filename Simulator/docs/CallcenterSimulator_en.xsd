<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="https://cs.highfives.io" xmlns="https://cs.highfives.io" elementFormDefault="qualified">

<!-- Description of the call center model files -->

<xs:element name="CallCenterModel"><xs:complexType>
  <xs:sequence>
    <xs:element name="ModelName" type="xs:string"/>
    <xs:element name="ModelSimulatorVersion" type="xs:string"/>
    <xs:element name="ModelDescription" type="xs:string"/>
    <xs:element name="ModelDate" minOccurs="0" maxOccurs="1"/>
    <xs:element name="ModelMaxQueueLength" type="xs:integer"/>
    <xs:element name="ModelSimulatedDays" type="xs:integer"/>
    <xs:element ref="ModelPreferredShiftLength"/>
	<xs:element ref="ModelMinimalShiftLength" minOccurs="0" maxOccurs="1"/>
    <xs:element name="ModelServiceLevel" type="xs:integer"/>
	<xs:element name="ModelProductivity" type="xs:string" minOccurs="0" maxOccurs="1"/>
	<xs:element name="ModelDiseaseRelatedSurchage" type="xs:string" minOccurs="0" maxOccurs="1"/>
    <xs:element ref="ModelClientType" minOccurs="0" maxOccurs="unbounded"/>
    <xs:element ref="ModelCallCenter" minOccurs="0" maxOccurs="unbounded"/>
    <xs:element ref="ModelSkillLevel" minOccurs="0" maxOccurs="unbounded"/>
	<xs:element ref="ModelThresholds" minOccurs="0" maxOccurs="1"/>
  </xs:sequence>
</xs:complexType></xs:element>

<xs:element name="ModelPreferredShiftLength" type="xs:integer"/>

<xs:element name="ModelMinimalShiftLength" type="xs:integer"/>

<xs:element name="ModelClientType"><xs:complexType>
  <xs:sequence>
    <xs:element ref="ClientsFreshCalls"/>
    <xs:element ref="ClientsScore"/>
    <xs:choice>
      <xs:element name="ClientsWaitingTimeToleranceDistribution" type="xs:string"/>
	  <xs:element name="ClientsWaitingTimeToleranceDistributionLong" type="xs:string"/>
      <xs:element ref="ClientsWaitingTimeToleranceEstimation"/>
    </xs:choice>
    <xs:element ref="ClientsRetry"/>
    <xs:element ref="ClientsForwarding"/>
	<xs:element ref="ClientsRecall"  minOccurs="0" maxOccurs="1"/>
    <xs:element name="YieldPerClient" type="xs:string"/>
    <xs:element ref="CostPerCaller"/>
  </xs:sequence>
  <xs:attribute name="Active"><xs:simpleType>
    <xs:restriction base="xs:NMTOKEN">
      <xs:enumeration value="0"/>
      <xs:enumeration value="1"/>
    </xs:restriction>
  </xs:simpleType></xs:attribute>
  <xs:attribute name="BlocksLine"><xs:simpleType>
    <xs:restriction base="xs:NMTOKEN">
      <xs:enumeration value="0"/>
      <xs:enumeration value="1"/>
    </xs:restriction>
  </xs:simpleType></xs:attribute>
  <xs:attribute name="ServiceLevel" type="xs:string"/>
  <xs:attribute name="Name" type="xs:string" use="required"/>
</xs:complexType></xs:element>

<xs:element name="ClientsFreshCalls"><xs:complexType>
  <xs:sequence>
	<xs:element ref="FreshCallsCount" minOccurs="1" maxOccurs="1"/>
    <xs:element name="FreshCallsDistribution" type="xs:string"/>
  </xs:sequence>
</xs:complexType></xs:element>

<xs:element name="FreshCallsCount"><xs:complexType>
  <xs:simpleContent>
    <xs:extension base="xs:string">
      <xs:attribute name="StandardDeviation" type="xs:string"/>
    </xs:extension>
  </xs:simpleContent>
</xs:complexType></xs:element>

<xs:element name="ClientsScore"><xs:complexType>
  <xs:attribute name="Base" type="xs:string" use="required"/>
  <xs:attribute name="PerWaitingSecond" type="xs:string" use="required"/>
  <xs:attribute name="Forwarding" type="xs:string" use="required"/>
</xs:complexType></xs:element>

<xs:element name="ClientsWaitingTimeToleranceEstimation"><xs:complexType>
  <xs:attribute name="WaitingTime" type="xs:string" use="required"/>
  <xs:attribute name="CancelRate" type="xs:string" use="required"/>
  <xs:attribute name="Correction" type="xs:string" use="required"/>
</xs:complexType></xs:element>

<xs:element name="ClientsRetry"><xs:complexType>
  <xs:sequence>
    <xs:element name="RetryIntervalDistribution" type="xs:string"/>
    <xs:element name="RetryProbabilityAfterBeingBlockedFirstRetry" type="xs:string"/>
    <xs:element name="RetryProbabilityAfterBeingBlockedFurtherRetrys" type="xs:string"/>
    <xs:element name="RetryProbabilityAfterCanceledFirstRetry" type="xs:string"/>
    <xs:element name="RetryProbabilityAfterCanceledFurtherRetrys" type="xs:string"/>
    <xs:element ref="RetryClientTypeAfterBeingBlockedFirstRetry"/>
    <xs:element ref="RetryClientTypeAfterBeingBlockedFurtherRetrys"/>
    <xs:element ref="RetryClientTypeAfterCanceledFirstRetry"/>
    <xs:element ref="RetryClientTypeAfterCanceledFurtherRetrys"/>
  </xs:sequence>
</xs:complexType></xs:element>

<xs:element name="RetryClientTypeAfterBeingBlockedFirstRetry"><xs:complexType>
  <xs:sequence>
    <xs:element ref="RetryNewClientType" minOccurs="0" maxOccurs="unbounded"/>
  </xs:sequence>
</xs:complexType></xs:element>

<xs:element name="RetryClientTypeAfterBeingBlockedFurtherRetrys"><xs:complexType>
  <xs:sequence>
    <xs:element ref="RetryNewClientType" minOccurs="0" maxOccurs="unbounded"/>
  </xs:sequence>
</xs:complexType></xs:element>

<xs:element name="RetryClientTypeAfterCanceledFirstRetry"><xs:complexType>
  <xs:sequence>
    <xs:element ref="RetryNewClientType" minOccurs="0" maxOccurs="unbounded"/>
  </xs:sequence>
</xs:complexType></xs:element>

<xs:element name="RetryClientTypeAfterCanceledFurtherRetrys"><xs:complexType>
  <xs:sequence>
    <xs:element ref="RetryNewClientType" minOccurs="0" maxOccurs="unbounded"/>
  </xs:sequence>
</xs:complexType></xs:element>

<xs:element name="RetryNewClientType"><xs:complexType>
  <xs:simpleContent>
    <xs:extension base="xs:string">
      <xs:attribute name="Name" type="xs:string" use="required"/>
    </xs:extension>
  </xs:simpleContent>
</xs:complexType></xs:element>

<xs:element name="ClientsForwarding"><xs:complexType>
  <xs:sequence>
    <xs:element ref="ForwardingNewClientType" minOccurs="0" maxOccurs="unbounded"/>
    <xs:element ref="ForwardingSkillLevelDepending" minOccurs="0" maxOccurs="unbounded"/>
  </xs:sequence>
  <xs:attribute name="Probability" type="xs:string" use="required"/>
</xs:complexType></xs:element>

<xs:element name="ForwardingNewClientType"><xs:complexType>
  <xs:simpleContent>
    <xs:extension base="xs:string">
      <xs:attribute name="Name" type="xs:string" use="required"/>
    </xs:extension>
  </xs:simpleContent>
</xs:complexType></xs:element>

<xs:element name="ForwardingSkillLevelDepending"><xs:complexType>
  <xs:sequence>
    <xs:element ref="ForwardingNewClientType" minOccurs="0" maxOccurs="unbounded"/>
  </xs:sequence>
  <xs:attribute name="SkillLevel" type="xs:string" use="required"/>
  <xs:attribute name="Probability" type="xs:string" use="required"/>
</xs:complexType></xs:element>

<xs:element name="ClientsRecall"><xs:complexType>
  <xs:sequence>
    <xs:element name="RecallIntervalDistribution" type="xs:string"/>
    <xs:element ref="RecallNewClientType" minOccurs="0" maxOccurs="unbounded"/>
    <xs:element ref="RecallSkillLevelDepending" minOccurs="0" maxOccurs="unbounded"/>
  </xs:sequence>
  <xs:attribute name="Probability" type="xs:string" use="required"/>
</xs:complexType></xs:element>

<xs:element name="RecallNewClientType"><xs:complexType>
  <xs:simpleContent>
    <xs:extension base="xs:string">
      <xs:attribute name="Name" type="xs:string" use="required"/>
    </xs:extension>
  </xs:simpleContent>
</xs:complexType></xs:element>

<xs:element name="RecallSkillLevelDepending"><xs:complexType>
  <xs:sequence>
    <xs:element ref="RecallNewClientType" minOccurs="0" maxOccurs="unbounded"/>
  </xs:sequence>
  <xs:attribute name="SkillLevel" type="xs:string" use="required"/>
  <xs:attribute name="Probability" type="xs:string" use="required"/>
</xs:complexType></xs:element>

<xs:element name="CostPerCaller"><xs:complexType>
  <xs:attribute name="Canceling" type="xs:string" use="required"/>
  <xs:attribute name="WaitingSecond" type="xs:string" use="required"/>
</xs:complexType></xs:element>

<xs:element name="ModelCallCenter"><xs:complexType>
  <xs:sequence>
    <xs:element ref="CallCenterAgentsGroup" minOccurs="0" maxOccurs="unbounded"/>
    <xs:element ref="CallCenterTechnicalFreeTime"/>
    <xs:element name="CallCenterScore" type="xs:string"/>
    <xs:element ref="CallCenterAgentsScore"/>
    <xs:element ref="CallCenterMinimumWaitingTime" minOccurs="0" maxOccurs="unbounded"/>
	<xs:element name="ModelProductivity" type="xs:string" minOccurs="0" maxOccurs="1"/>
	<xs:element name="ModelDiseaseRelatedSurchage" type="xs:string" minOccurs="0" maxOccurs="1"/>
  </xs:sequence>
  <xs:attribute name="Active"><xs:simpleType>
    <xs:restriction base="xs:NMTOKEN">
      <xs:enumeration value="0"/>
      <xs:enumeration value="1"/>
    </xs:restriction>
  </xs:simpleType></xs:attribute>
  <xs:attribute name="Name" type="xs:string" use="required"/>
</xs:complexType></xs:element>

<xs:element name="CallCenterTechnicalFreeTime"><xs:complexType>
  <xs:simpleContent>
    <xs:extension base="xs:integer">
      <xs:attribute name="IsWaitingTime" type="xs:string"/>
    </xs:extension>
  </xs:simpleContent>
</xs:complexType></xs:element>

<xs:element name="CallCenterAgentsScore"><xs:complexType>
  <xs:sequence>
    <xs:element name="AgentsScoreFactorSinceLastCall" type="xs:string"/>
    <xs:element name="AgentsScoreFactorFreeTimePart" type="xs:string"/>
  </xs:sequence>
</xs:complexType></xs:element>

<xs:element name="CallCenterAgentsGroup"><xs:complexType>
  <xs:sequence>
    <xs:choice>
	  <xs:sequence>
        <xs:element name="AgentsGroupCount" type="xs:string"/>
        <xs:element name="AgentsGroupWorkingTimeBegin" type="xs:string"/>
        <xs:element name="AgentsGroupWorkingTimeEnd" type="xs:string"/>
      </xs:sequence>
	  <xs:choice>
        <xs:element ref="AgentsGroupDistribution"/>
      </xs:choice>
      <xs:choice>
        <xs:element ref="AgentsGroupByClientArrivals"/>
      </xs:choice>
    </xs:choice>
    <xs:element name="AgentsGroupSkillLevel" type="xs:string"/>
    <xs:element name="CostsPerHour" type="xs:string"/>
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="CostsPerClientType"/>
    </xs:choice>
    <xs:element ref="ModelPreferredShiftLength" minOccurs="0" maxOccurs="1"/>
	<xs:element ref="ModelMinimalShiftLength"  minOccurs="0" maxOccurs="1"/>
	<xs:element name="ModelProductivity" type="xs:string" minOccurs="0" maxOccurs="1"/>
	<xs:element name="ModelDiseaseRelatedSurchage" type="xs:string" minOccurs="0" maxOccurs="1"/>
  </xs:sequence>
  <xs:attribute name="Active"><xs:simpleType>
    <xs:restriction base="xs:NMTOKEN">
      <xs:enumeration value="0"/>
      <xs:enumeration value="1"/>
    </xs:restriction>
  </xs:simpleType></xs:attribute>
</xs:complexType></xs:element>

<xs:element name="AgentsGroupDistribution"><xs:complexType>
  <xs:simpleContent>
    <xs:extension base="xs:string">
      <xs:attribute name="LastShiftIsOpenEnd"><xs:simpleType>
        <xs:restriction base="xs:NMTOKEN">
          <xs:enumeration value="0"/>
          <xs:enumeration value="1"/>
        </xs:restriction>
      </xs:simpleType></xs:attribute>
    </xs:extension>
  </xs:simpleContent>
</xs:complexType></xs:element>

<xs:element name="AgentsGroupByClientArrivals"><xs:complexType>
  <xs:sequence>
    <xs:element ref="ByClientArrivalsGroup" minOccurs="0" maxOccurs="unbounded"/>
  </xs:sequence>
  <xs:attribute name="AgentsHalfHours" type="xs:string" use="required"/>
</xs:complexType></xs:element>

<xs:element name="ByClientArrivalsGroup"><xs:complexType>
  <xs:simpleContent>
    <xs:extension base="xs:string">
      <xs:attribute name="Rate" type="xs:string" use="required"/>
    </xs:extension>
  </xs:simpleContent>
</xs:complexType></xs:element>

<xs:element name="CostsPerClientType"><xs:complexType>
  <xs:attribute name="ClientType" type="xs:string" use="required"/>
  <xs:attribute name="PerCall" type="xs:string" use="required"/>
  <xs:attribute name="PerMinute" type="xs:string" use="required"/>
</xs:complexType></xs:element>
	
<xs:element name="CallCenterMinimumWaitingTime"><xs:complexType>
  <xs:sequence>
    <xs:element ref="MinimumWaitingTimeClientType" minOccurs="0" maxOccurs="unbounded"/>
  </xs:sequence>
</xs:complexType></xs:element>

<xs:element name="MinimumWaitingTimeClientType"><xs:complexType>
  <xs:simpleContent>
    <xs:extension base="xs:string">
      <xs:attribute name="Name" type="xs:string" use="required"/>
    </xs:extension>
  </xs:simpleContent>
</xs:complexType></xs:element>
	
<xs:element name="ModelSkillLevel"><xs:complexType>
  <xs:sequence>
    <xs:element ref="SkillLevelClientType" minOccurs="0" maxOccurs="unbounded"/>
  </xs:sequence>
  <xs:attribute name="Name" type="xs:string" use="required"/>
</xs:complexType></xs:element>

<xs:element name="SkillLevelClientType"><xs:complexType>
  <xs:sequence>
    <xs:element name="SkillLevelClientTypeHoldingTimeAddOn" minOccurs="0" maxOccurs="unbounded"/>
    <xs:element name="SkillLevelClientTypeHoldingTimeDistribution" minOccurs="1" maxOccurs="unbounded"/>
    <xs:element name="SkillLevelClientTypePostProcessingTimeDistribution" minOccurs="1" maxOccurs="unbounded"/>
    <xs:element name="SkillLevelClientTypeScore" type="xs:string"/>
  </xs:sequence>
  <xs:attribute name="Name" type="xs:string" use="required"/>
</xs:complexType></xs:element>

<xs:element name="SkillLevelClientTypeHoldingTimeAddOn"><xs:complexType>
  <xs:simpleContent>
    <xs:extension base="xs:string">
      <xs:attribute name="Interval" type="xs:string"/>
    </xs:extension>
  </xs:simpleContent>
</xs:complexType></xs:element>

<xs:element name="SkillLevelClientTypeHoldingTimeDistribution"><xs:complexType>
  <xs:simpleContent>
    <xs:extension base="xs:string">
      <xs:attribute name="Interval" type="xs:string"/>
    </xs:extension>
  </xs:simpleContent>
</xs:complexType></xs:element>

<xs:element name="SkillLevelClientTypePostProcessingTimeDistribution"><xs:complexType>
  <xs:simpleContent>
    <xs:extension base="xs:string">
      <xs:attribute name="Interval" type="xs:string"/>
    </xs:extension>
  </xs:simpleContent>
</xs:complexType></xs:element>

<xs:element name="ModelThresholds"><xs:complexType>
  <xs:sequence>
    <xs:element ref="ThresholdData" minOccurs="0" maxOccurs="unbounded"/>
  </xs:sequence>
</xs:complexType></xs:element>

<xs:element name="ThresholdData"><xs:complexType>
  <xs:attribute name="Type" type="xs:string" use="required"/>
  <xs:attribute name="Intervals" type="xs:string" use="required"/>
  <xs:attribute name="Grpups" type="xs:string" use="required"/>
  <xs:attribute name="WarningYellow" type="xs:string" use="required"/>
  <xs:attribute name="WarningRed" type="xs:string" use="required"/>
  <xs:attribute name="Value" type="xs:string"/>
  <xs:attribute name="Status" type="xs:string"/>
</xs:complexType></xs:element>

<!-- Description of the statistic files -->

<xs:element name="Statistic"><xs:complexType>
  <xs:sequence>
    <xs:element ref="CallCenterModel"/>
    <xs:element name="StatisticRunDate" type="xs:string"/>
    <xs:element name="StatisticUser" type="xs:string"  minOccurs="0" maxOccurs="1"/>
	<xs:element name="StatisticServer" type="xs:string" minOccurs="0" maxOccurs="1"/>
	<xs:element name="StatisticServerOS" type="xs:string" minOccurs="0" maxOccurs="1"/>
    <xs:element name="StatisticRunTime" type="xs:integer"/>
    <xs:element name="StatisticThreads" type="xs:integer"/>
    <xs:element name="StatisticDays" type="xs:integer"/>
    <xs:element name="StatisticEvents" type="xs:integer"/>
    <xs:element ref="StatisticClients" minOccurs="0" maxOccurs="unbounded"/>
    <xs:element ref="StatisticAgents" minOccurs="0" maxOccurs="unbounded"/>
	<xs:element name="StatisticRevenueSummary" type="xs:string" minOccurs="0" maxOccurs="1"/>
    <xs:element ref="StatisticQueue"/>
	<xs:element ref="StatisticQueueSummary" minOccurs="0" maxOccurs="unbounded"/>	
	<xs:element ref="StatisticModelAgents" minOccurs="0" maxOccurs="unbounded"/>
    <xs:element ref="ErlangCWithoutRetry" minOccurs="0" maxOccurs="1"/>
    <xs:element ref="ErlangCWithRetry" minOccurs="0" maxOccurs="1"/>
    <xs:element ref="StatisticThresholds" minOccurs="0" maxOccurs="1"/>
  </xs:sequence>
</xs:complexType></xs:element>

<xs:element name="StatisticClients"><xs:complexType>
  <xs:sequence>    
    <xs:element ref="StatisticClientsCount"/>
	<xs:element ref="StatisticClientsSummary" minOccurs="0" maxOccurs="unbounded"/>
	<xs:element ref="StatisticClientsCarryOver"/>
    <xs:element ref="StatisticClientsWaitingTime"/>
    <xs:element ref="StatisticClientsHoldingTime"/>
    <xs:element ref="StatisticClientsCancelTime"/>
    <xs:element ref="StatisticClientsServiceLevel"/>
    <xs:element ref="StatisticClientsForwarding"/>
	<xs:element ref="StatisticClientsRetry"/>
	<xs:element ref="StatisticClientsRecall"/>
    <xs:element ref="StatisticClientsWaitingTimeDistribution"/>
    <xs:element ref="StatisticClientsHoldingTimeDistribution"/>
    <xs:element ref="StatisticClientsCancelTimeDistribution"/>
	<xs:element ref="StatisticClientsWaitingTimeDistributionLong"/>
    <xs:element ref="StatisticClientsHoldingTimeDistributionLong"/>
    <xs:element ref="StatisticClientsCancelTimeDistributionLong"/>
    <xs:element ref="StatisticCallsCount"/>
    <xs:element ref="StatisticCallsWaitingTime"/>
    <xs:element ref="StatisticCallsHoldingTime"/>
    <xs:element ref="StatisticCallsCancelTime"/>
    <xs:element ref="StatisticCallsServiceLevel"/>
    <xs:element ref="StatisticCallsForwarding"/>
	<xs:element ref="StatisticCallsRetry"/>
    <xs:element ref="StatisticCallsWaitingTimeDistribution"/>
    <xs:element ref="StatisticCallsHoldingTimeDistribution"/>
    <xs:element ref="StatisticCallsCancelTimeDistribution"/>
	<xs:element ref="StatisticCallsWaitingTimeDistributionLong"/>
    <xs:element ref="StatisticCallsHoldingTimeDistributionLong"/>
    <xs:element ref="StatisticCallsCancelTimeDistributionLong"/>
    <xs:element ref="StatisticConfidence"/>
    <xs:element ref="StatisticCallsCosts"/>
  </xs:sequence>
  <xs:attribute name="Name" type="xs:string"/>
</xs:complexType></xs:element>

<xs:element name="StatisticClientsSummary"><xs:complexType>
  <xs:attribute name="Range" type="xs:string"/>
  <xs:attribute name="ClientsAccessibility" type="xs:string" use="required"/>
  <xs:attribute name="CallsAccessibility" type="xs:string" use="required"/>
  <xs:attribute name="ClientsAverageWaitingTime" type="xs:string" use="required"/>
  <xs:attribute name="CallsAverageWaitingTime" type="xs:string" use="required"/>
  <xs:attribute name="ClientsAverageResidenceTime" type="xs:string"/>
  <xs:attribute name="CallsAverageResidenceTime" type="xs:string"/>
  <xs:attribute name="ClientsServiceLevel" type="xs:string" use="required"/>
  <xs:attribute name="ClientsServiceLevelAll" type="xs:string"/>
  <xs:attribute name="CallsServiceLevel" type="xs:string" use="required"/>
  <xs:attribute name="CallsServiceLevelAll" type="xs:string"/>
  <xs:attribute name="ClientsAverageCancelTime" type="xs:string"/>
  <xs:attribute name="CallsAverageCancelTime" type="xs:string"/>
  <xs:attribute name="CallsCombinedAverageWaitingAndCancelTimeTime" type="xs:string"/>
  <xs:attribute name="ClientsCombinedAverageWaitingAndCancelTimeTime" type="xs:string"/>
</xs:complexType></xs:element>

<xs:element name="StatisticClientsCount"><xs:complexType>
  <xs:attribute name="Count" type="xs:string" use="required"/>
  <xs:attribute name="Success" type="xs:string" use="required"/>
  <xs:attribute name="Blocked" type="xs:string" use="required"/>
  <xs:attribute name="Canceled" type="xs:string" use="required"/>
  <xs:attribute name="CarryOver" type="xs:string" use="required"/>
  <xs:attribute name="CountPerInterval" type="xs:string" use="required"/>
  <xs:attribute name="SuccessPerInterval" type="xs:string" use="required"/>
  <xs:attribute name="BlockedPerInterval" type="xs:string" use="required"/>
  <xs:attribute name="CanceledPerInterval" type="xs:string" use="required"/>
</xs:complexType></xs:element>

<xs:element name="StatisticClientsCarryOver"><xs:complexType>
  <xs:sequence>
    <xs:element name="StatisticClientsCanceledPerDay" type="xs:string"/>
    <xs:element name="StatisticClientsRetryTimesPerDay" type="xs:string"/>
	<xs:element name="StatisticClientsWaitingTimeTolerancePerDay" type="xs:string"/>
  </xs:sequence>
</xs:complexType></xs:element>

<xs:element name="StatisticCallsCount"><xs:complexType>
  <xs:attribute name="Count" type="xs:string" use="required"/>
  <xs:attribute name="Success" type="xs:string" use="required"/>
  <xs:attribute name="Blocked" type="xs:string" use="required"/>
  <xs:attribute name="Canceled" type="xs:string" use="required"/>
  <xs:attribute name="CarryOver" type="xs:string" use="required"/>
  <xs:attribute name="CountPerInterval" type="xs:string" use="required"/>
  <xs:attribute name="SuccessPerInterval" type="xs:string" use="required"/>
  <xs:attribute name="BlockedPerInterval" type="xs:string" use="required"/>
  <xs:attribute name="CanceledPerInterval" type="xs:string" use="required"/>
</xs:complexType></xs:element>

<xs:element name="StatisticClientsWaitingTime"><xs:complexType>
  <xs:attribute name="Sum" type="xs:string" use="required"/>
  <xs:attribute name="SquaresSum" type="xs:string" use="required"/>
  <xs:attribute name="SumPerInterval" type="xs:string" use="required"/>
</xs:complexType></xs:element>

<xs:element name="StatisticCallsWaitingTime"><xs:complexType>
  <xs:attribute name="Sum" type="xs:string" use="required"/>
  <xs:attribute name="SquaresSum" type="xs:string" use="required"/>
  <xs:attribute name="SumPerInterval" type="xs:string" use="required"/>
</xs:complexType></xs:element>

<xs:element name="StatisticClientsHoldingTime"><xs:complexType>
  <xs:attribute name="Sum" type="xs:string" use="required"/>
  <xs:attribute name="SquaresSum" type="xs:string" use="required"/>
  <xs:attribute name="SumPerInterval" type="xs:string" use="required"/>
</xs:complexType></xs:element>

<xs:element name="StatisticCallsHoldingTime"><xs:complexType>
  <xs:attribute name="Sum" type="xs:string" use="required"/>
  <xs:attribute name="SquaresSum" type="xs:string" use="required"/>
  <xs:attribute name="SumPerInterval" type="xs:string" use="required"/>
</xs:complexType></xs:element>

<xs:element name="StatisticClientsCancelTime"><xs:complexType>
  <xs:attribute name="Sum" type="xs:string" use="required"/>
  <xs:attribute name="SquaresSum" type="xs:string" use="required"/>
  <xs:attribute name="SumPerInterval" type="xs:string" use="required"/>
</xs:complexType></xs:element>

<xs:element name="StatisticCallsCancelTime"><xs:complexType>
  <xs:attribute name="Sum" type="xs:string" use="required"/>
  <xs:attribute name="SquaresSum" type="xs:string" use="required"/>
  <xs:attribute name="SumPerInterval" type="xs:string" use="required"/>
</xs:complexType></xs:element>

<xs:element name="StatisticClientsServiceLevel"><xs:complexType>
  <xs:attribute name="Count" type="xs:string" use="required"/>
  <xs:attribute name="CountPerInterval" type="xs:string" use="required"/>
</xs:complexType></xs:element>

<xs:element name="StatisticCallsServiceLevel"><xs:complexType>
  <xs:attribute name="Count" type="xs:string" use="required"/>
  <xs:attribute name="CountPerInterval" type="xs:string" use="required"/>
</xs:complexType></xs:element>

<xs:element name="StatisticClientsForwarding"><xs:complexType>
  <xs:attribute name="Count" type="xs:string" use="required"/>
  <xs:attribute name="CountPerInterval" type="xs:string" use="required"/>
</xs:complexType></xs:element>

<xs:element name="StatisticClientsRetry"><xs:complexType>
  <xs:attribute name="Count" type="xs:string" use="required"/>
  <xs:attribute name="CountPerInterval" type="xs:string" use="required"/>
</xs:complexType></xs:element>

<xs:element name="StatisticClientsRecall"><xs:complexType>
